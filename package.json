{
  "name": "propresenter-presentation-builder",
  "version": "0.0.1",
  "description": "A tool to automatically build ProPresenter presentations from simple input",
  "productName": "ProPresenter Presentation Builder",
  "author": "Charles Garwood <cgarwood@gmail.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue ./",
    "format": "prettier --write \"**/*.{js,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev -m electron '--' --enable-logging",
    "build": "quasar build -m electron"
  },
  "dependencies": {
    "@electron/remote": "^2.1.1",
    "@fontsource/open-sans": "^5.0.19",
    "@quasar/extras": "^1.16.4",
    "pinia": "^2.0.11",
    "protobufjs": "^7.2.6",
    "quasar": "^2.14.2",
    "uuid": "^9.0.1",
    "vue": "^3.4.15",
    "vue-router": "^4.0.0",
    "vue3-smooth-dnd": "^0.0.6"
  },
  "devDependencies": {
    "@quasar/app-vite": "^1.7.3",
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "autoprefixer": "^10.4.17",
    "electron": "^28.1.4",
    "electron-packager": "^17.1.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.20.1",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
    "typescript": "^5.2.2"
  },
  "resolutions": {
    "vite": "^5.0.7",
    "@vitejs/plugin-vue": "^4.5.2"
  },
  "engines": {
    "node": "^20 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
