    "uuid": "^9.0.1",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0",
    "vue3-smooth-dnd": "^0.0.6",
    "youversion-suggest": "^2.0.1"
  },
  "devDependencies": {
    "@quasar/app-vite": "^1.3.0",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "autoprefixer": "^10.4.2",
    "electron": "^28.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^8.1.0",
